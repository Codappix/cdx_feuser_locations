{
    "name": "web-vision/wv_feuser_locations",
    "type": "typo3-cms-extension",
    "description": "Extend fe_users with locations.",
    "homepage": "http://web-vision.de",
    "license": ["GPL-2.0+"],
    "autoload": {
        "psr-4": {
            "WebVision\\WvFeuserLocations\\": "Classes"
        }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1",
        "typo3/cms": "~7.6"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "web"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebVision\\WvFeuserLocations\\Tests\\": "Tests/",
            "TYPO3\\CMS\\Core\\": "vendor/typo3/cms/typo3/sysext/core/Classes/",
            "TYPO3\\CMS\\Core\\Tests\\": "vendor/typo3/cms/typo3/sysext/core/Tests/",
            "TYPO3\\CMS\\Frontend\\": "vendor/typo3/cms/typo3/sysext/frontend/Classes/"
        }
    }
}
